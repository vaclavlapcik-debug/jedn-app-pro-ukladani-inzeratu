diff --git a/web/src/index.css b/web/src/index.css
--- a/web/src/index.css
+++ b/web/src/index.css
@@
 .price-counter {
   font-variant-numeric: tabular-nums;
   transition: color 0.4s ease;
   font-weight: 800;
   letter-spacing: -0.02em;
 }
+
+/* ================================
+   Brand SVG “pencil coloring” hover
+   ================================ */
+.brand{ display:inline-flex; align-items:center; justify-content:center; cursor:pointer; }
+.logo{ width:48px; height:48px; display:block; }
+
+/* Outline stays visible */
+.logo .outline{ stroke:currentColor; fill:none; }
+
+/* Pencil hatch fill (revealed by mask rect) */
+.logo .fill{ opacity:0.95; }
+
+/* SVG transforms need transform-box for consistent behavior */
+.logo .revealRect{
+  transform-box: fill-box;
+  transform-origin: 0% 50%;
+  transform: scaleX(0);
+  transition: transform 900ms cubic-bezier(.22,1,.36,1);
+}
+
+.brand:hover .logo .revealRect,
+.brand:focus-visible .logo .revealRect,
+.brand:focus-within .logo .revealRect{
+  transform: scaleX(1);
+}
+
+@media (prefers-reduced-motion: reduce){
+  .logo .revealRect{ transition:none; transform: scaleX(1); }
+}
diff --git a/web/src/components/BrandIcon.tsx b/web/src/components/BrandIcon.tsx
new file mode 100644
--- /dev/null
+++ b/web/src/components/BrandIcon.tsx
@@
+import React from "react";
+
+type Props = {
+  Logo: React.FC<React.SVGProps<SVGSVGElement>>;
+  name: string;
+  color?: string;
+  size?: number;
+  tabIndex?: number;
+};
+
+export function BrandIcon({
+  Logo,
+  name,
+  color = "#111",
+  size = 48,
+  tabIndex = 0,
+}: Props) {
+  return (
+    <span
+      className="brand"
+      title={name}
+      style={{ color, width: size, height: size }}
+      tabIndex={tabIndex}
+      aria-label={name}
+    >
+      <Logo className="logo" width={size} height={size} />
+    </span>
+  );
+}
